    <include file="$(find gki_zerg_bringup)/launch/3dsensor.launch">
        <arg name="scan_processing" value="false" />
        <arg name="depth_registration" value="false" />
    </include>
    <include file="$(find gki_zerg_bringup)/launch/master_discovery.launch" />
-->
</launch>

